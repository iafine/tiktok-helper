{
  "name": "tiktok-helper",
  "displayName": "__MSG_extensionName__",
  "version": "0.0.0",
  "description": "__MSG_extensionDescription__",
  "author": "topaz",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@plasmohq/storage": "^0.7.0",
    "antd": "4.21.3",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "plasmo": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "tailwindcss": "3.1.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chrome": "0.0.190",
    "@types/node": "17.0.43",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "postcss": "8.4.14",
    "prettier": "2.7.0",
    "typescript": "4.7.3"
  },
  "manifest": {
    "default_locale": "en",
    "homepage_url": "https://www.chipfly.co",
    "permissions": [
      "tabs",
      "activeTab",
      "contextMenus",
      "commands"
    ],
    "host_permissions": [
      "https://www.tiktok.com/*",
      "https://api.tiktokv.com/*"
    ],
    "action": {
      "default_title": "__MSG_actionDefaultTitle__"
    },
    "commands": {
      "open_parse_video": {
        "suggested_key": {
          "default": "Ctrl+Shift+K",
          "mac": "Command+Shift+K"
        },
        "description": "__MSG_commandsParseVideoDescription__"
      }
    }
  }
}
